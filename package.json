    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1",
    "yargs": "1.3.3"
  },
  "dependencies": {
    "clappr-zepto": "0.0.4",
    "hls.js": "dailymotion/hls.js",
    "lodash.assign": "^3.2.0",
    "lodash.find": "^3.2.1",
    "lodash.once": "^3.0.1",
    "lodash.result": "^3.1.2",
    "lodash.uniq": "^3.2.2"
  }
