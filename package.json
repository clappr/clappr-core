  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/clappr/clappr/issues"
  },
  "homepage": "https://github.com/clappr/clappr",
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "clean-webpack-plugin": "^0.1.3",
    "coveralls": "^2.11.4",
    "css-loader": "^0.19.0",
    "dotenv": "^1.2.0",
    "file-loader": "^0.8.4",
    "gulp": "^3.8.1",
    "html-loader": "^0.3.0",
    "isparta": "^3.0.3",
