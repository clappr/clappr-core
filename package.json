{
  "name": "clappr",
  "version": "0.2.13",
  "description": "An extensible media player for the web",
  "main": "dist/clappr.min.js",
  "scripts": {
    "release": "node_modules/.bin/webpack --progress -d -p --optimize-minimize --optimize-dedupe --output-filename clappr.min.js",
    "build": "node_modules/.bin/webpack --progress",
    "watch": "node_modules/.bin/webpack --progress --watch",
    "test": "node_modules/.bin/karma start --single-run --browsers Chrome",
    "start": "node_modules/.bin/webpack-dev-server --content-base public/ --output-public-path latest/ --hot",
    "lock": "rm -rf npm-shrinkwrap.json node_modules && npm install --silent && npm shrinkwrap"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:clappr/clappr.git"
  },
  "author": "Globo.com",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/clappr/clappr/issues"
  },
  "browser": {
    "zepto": "clappr-zepto"
  },
  "homepage": "https://github.com/clappr/clappr",
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-loader": "^5.3.2",
    "bower": "1.3.12",
    "chai": "^3.2.0",
    "clean-webpack-plugin": "^0.1.3",
    "compass-mixins": "^0.12.7",
    "coveralls": "^2.11.4",
    "css-loader": "^0.17.0",
    "dotenv": "^1.2.0",
    "express-alias": "0.4.0",
    "file-loader": "^0.8.4",
    "gulp": "^3.8.1",
    "html-loader": "^0.3.0",
    "isparta": "^3.0.3",
    "istanbul": "^0.3.19",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.3",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.0",
    "karma-cli": "^0.1.0",
    "karma-coverage": "^0.5.2",
    "karma-firefox-launcher": "^0.1.6",
    "karma-jquery": "^0.1.0",
    "karma-mocha": "^0.2.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sinon": "^1.0.3",
    "karma-sinon-chai": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "mkdirp": "^0.5.1",
